{
  "name": "chromatechnology/statamic-html-field",
  "description": "A Statamic fieldtype that allows to pass HTML code to the frontend (inline).",
  "version": "2.2.0",
  "keywords": [
      "statamic",
      "fieldtype",
      "html",
      "chromatech"
  ],
  "require": {
    "php": "^8.1 || ^8.2",
    "illuminate/support": "^9.0 || ^10.0 || ^11.0 || ^12.0",
    "statamic/cms": "^3.0 || ^4.0 || ^5.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
        "ChromaTech\\HtmlField\\": "src"
    }
  },
  "authors": [
    {
      "name": "ChromaTech",
      "role": "Developer"
    }
  ],
  "extra": {
    "statamic": {
      "name": "HTML Field",
      "description": "A Statamic fieldtype that allows to pass HTML code to the frontend (inline)."
    },
    "laravel": {
      "providers": [
          "ChromaTech\\HtmlField\\ServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "pixelfear/composer-dist-plugin": true
    }
  }
}
